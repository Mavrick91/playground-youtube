{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "prettier",
    "plugin:@typescript-eslint/recommended"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-console": [
      1,
      {
        "allow": [
          "error"
        ]
      }
    ],
    "camelcase": [
      2,
      {
        "allow": [
          "youtube_v3",
          "client_id",
          "redirect_uri",
          "caption_label",
          "nav_button",
          "nav_button_previous",
          "nav_button_next",
          "head_row",
          "head_cell",
          "access_token",
          "day_range_end",
          "day_selected",
          "day_today",
          "day_outside",
          "day_disabled",
          "day_range_middle",
          "day_hidden",
          "people_v1"
        ]
      }
    ],
    "react/react-in-jsx-scope": 0,
    "@typescript-eslint/no-unused-vars": [
      2
    ],
    "no-shadow": 0,
    "import/prefer-default-export": 0,
    "react/require-default-props": 0,
    "react/prop-types": 0,
    "import/extensions": [
      2,
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ],
    "react/jsx-props-no-spreading": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    ]
  }
}